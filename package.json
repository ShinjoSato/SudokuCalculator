{
  "name": "MultiSudokuCalculator",
  "version": "1.0.0",
  "description": "First launch",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-windows": "electron-packager . sudoku-application --platform=win32 --arch=x64 --overwrite",
    "build-mac": "electron-packager . sudoku-application --platform=darwin --arch=x64 --overwrite",
    "dist": "electron-builder"
  },
  "author": "ShinioS",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "appId": "合体数独計算機",
    "files": [
      "./main.js",
      "./src/*",
      "./src/images/*",
      "./src/js/*",
      "./src/package/*",
      "./build/appx/*"
    ],
    "win": {
      "icon": "build/appx/StoreLogo.png",
      "target": "appx"
    },
    "directories": {
      "buildResources": "build",
      "output": "build"
    },
    "appx": {
      "identityName": "23936Shinio.MultiSudokuCalculator",
      "applicationId": "Shinio.MultiSudokuCalculator",
      "publisherDisplayName": "ShinioS",
      "publisher": "CN=538D6282-CAEA-469B-B590-25C474567FA3",
      "displayName": "合体数独計算機",
      "languages":["JA-JP"]
    }
  }
}
